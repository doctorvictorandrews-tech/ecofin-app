{
  "version": 2,
  "builds": [
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/cliente",
      "dest": "/public/cliente.html"
    },
    {
      "src": "/painel",
      "dest": "/public/painel.html"
    }
  ]
}
